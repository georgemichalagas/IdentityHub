# Staging Environment Variables
ASPNETCORE_ENVIRONMENT=Staging

# API Configuration
API_PORT=7080

# Database Configuration
DB_HOST=identityhub-postgres-staging
DB_NAME=identityhub_staging
DB_USER=postgres
DB_PASSWORD=your_secure_staging_db_password
DB_PORT=5432

# JWT Configuration
JWT_SECRET_KEY=your_staging_jwt_secret_key_minimum_256_bits

# Email Configuration
SMTP_SERVER=smtp.sendgrid.net
SMTP_PORT=587
SMTP_USERNAME=apikey
SMTP_PASSWORD=your_sendgrid_api_key
FROM_EMAIL=noreply@staging.yourdomain.com

# CORS Origins (comma-separated)
CORS_ORIGINS=https://staging.yourdomain.com,https://staging-admin.yourdomain.com

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100

# Monitoring (Optional)
APPINSIGHTS_INSTRUMENTATIONKEY=your_application_insights_key
